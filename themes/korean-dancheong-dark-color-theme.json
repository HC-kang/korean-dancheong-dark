    "terminal.ansiBlack": "#171E1A",
    "terminal.ansiRed": "#F55C48",
    "terminal.ansiGreen": "#27A851",
    "terminal.ansiYellow": "#FAD01B",
    "terminal.ansiBlue": "#0D49BB",
    "terminal.ansiMagenta": "#B85C8A",
    "terminal.ansiCyan": "#4CBF6E",
    "terminal.ansiWhite": "#DAD2C8",
    "terminal.ansiBrightBlack": "#6A655C",
    "terminal.ansiBrightRed": "#FF6E5A",
    "terminal.ansiBrightGreen": "#3AB865",
    "terminal.ansiBrightYellow": "#FFDE3E",
    "terminal.ansiBrightBlue": "#4A8FD8",
    "terminal.ansiBrightMagenta": "#C878A2",
    "terminal.ansiBrightCyan": "#6FD488",
    "terminal.ansiBrightWhite": "#F5F0E8"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#7A7568" }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": { "foreground": "#9A9588" }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": { "foreground": "#FF6E5A" }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": { "foreground": "#C8BEB4" }
    },

    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": { "foreground": "#F55C48", "fontStyle": "bold" }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": { "foreground": "#4A8FD8" }
    },
    {
      "name": "Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": { "foreground": "#FAD01B" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": { "foreground": "#EAE2D8" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#4A8FD8", "fontStyle": "bold" }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": { "foreground": "#B85C8A" }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": { "foreground": "#FF6E5A" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": { "foreground": "#FAD01B" }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": { "foreground": "#4CBF6E" }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": { "foreground": "#5DCFA3" }
    },
    {
      "name": "Symbols",
      "scope": "constant.other.symbol",
      "settings": { "foreground": "#FAD01B" }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": { "foreground": "#C8BEB4" }
    },
    {
      "name": "HTML: Tags",
      "scope": ["meta.tag"],
      "settings": { "foreground": "#4A8FD8" }
    },
    {
      "name": "HTML: Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#5DCFA3" }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#FAD01B" }
    },
    {
      "name": "CSS: Values",
      "scope": ["support.constant.property-value"],
      "settings": { "foreground": "#4CBF6E" }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": { "foreground": "#F55C48", "fontStyle": "bold" }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": { "foreground": "#4A8FD8" }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": { "foreground": "#4CBF6E" }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": { "foreground": "#F55C48" }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": { "foreground": "#4A8FD8" }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#B85C8A",
    "class": { "foreground": "#B85C8A" },
    "interface": { "foreground": "#B85C8A" },
    "enum": { "foreground": "#FAD01B" },
    "enumMember": "#FAD01B",
    "typeParameter": "#4A8FD8",
    "parameter": "#EAE2D8",
    "property": "#EAE2D8",
    "variable.readonly": "#FAD01B",
    "function": { "foreground": "#4A8FD8", "bold": true },
    "method": "#4A8FD8",
    "macro": "#F55C48",
    "event": "#F55C48",
    "decorator": "#5DCFA3",
    "regexp": "#5DCFA3",
    "string": "#4CBF6E",
    "number": "#FAD01B",
    "comment": "#7A7568",
    "operator": "#C8BEB4",
    "keyword": { "foreground": "#F55C48", "bold": true },
    "modifier": "#F55C48",
    "deprecated": { "strikethrough": true }
  }
}
