    "editorBracketHighlight.foreground4": "#FAD01B",
    "editorBracketHighlight.foreground5": "#B85C8A",
    "editorBracketHighlight.foreground6": "#5DCFA3",

    "gitDecoration.addedResourceForeground": "#27A851",
