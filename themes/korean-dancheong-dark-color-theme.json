    "terminal.ansiRed": "#C84C3A",
    "terminal.ansiGreen": "#3FA45B",
    "terminal.ansiYellow": "#E0B62C",
    "terminal.ansiBlue": "#3F75B6",
    "terminal.ansiMagenta": "#B85C8A",
    "terminal.ansiCyan": "#4CBF6E",
