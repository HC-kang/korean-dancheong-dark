    "terminal.ansiBrightBlack": "#6A655C",
    "terminal.ansiBrightRed": "#FF6E5A",
    "terminal.ansiBrightGreen": "#3AB865",
    "terminal.ansiBrightYellow": "#FFDE3E",
    "terminal.ansiBrightBlue": "#4A8FD8",
    "terminal.ansiBrightMagenta": "#C878A2",
    "terminal.ansiBrightCyan": "#6FD488",
    "terminal.ansiBrightWhite": "#F5F0E8"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#7A7568" }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": { "foreground": "#9A9588" }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": { "foreground": "#FF6E5A" }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": { "foreground": "#C8BEB4" }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": { "foreground": "#C84C3A", "fontStyle": "bold" }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": { "foreground": "#3F75B6" }
    },
    {
      "name": "Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": { "foreground": "#E0B62C" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": { "foreground": "#EAE2D8" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#3F75B6", "fontStyle": "bold" }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.type", "entity.other.inherited-class", "support.class"],
      "settings": { "foreground": "#B85C8A" }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": { "foreground": "#C84C3A" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": { "foreground": "#E0B62C" }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": { "foreground": "#4CBF6E" }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": { "foreground": "#5DCFA3" }
    },
    {
      "name": "Symbols",
      "scope": "constant.other.symbol",
      "settings": { "foreground": "#E0B62C" }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": { "foreground": "#C8BEB4" }
    },
    {
      "name": "HTML: Tags",
      "scope": ["meta.tag"],
      "settings": { "foreground": "#3F75B6" }
    },
    {
      "name": "HTML: Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#5DCFA3" }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#E0B62C" }
    },
    {
      "name": "CSS: Values",
      "scope": ["support.constant.property-value"],
      "settings": { "foreground": "#4CBF6E" }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": { "foreground": "#C84C3A", "fontStyle": "bold" }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": { "foreground": "#3F75B6" }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": { "foreground": "#4CBF6E" }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": { "foreground": "#C84C3A" }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": { "foreground": "#3F75B6" }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#B85C8A",
    "class": { "foreground": "#B85C8A" },
    "interface": { "foreground": "#B85C8A" },
    "enum": { "foreground": "#E0B62C" },
    "enumMember": "#E0B62C",
    "typeParameter": "#3F75B6",
    "parameter": "#EAE2D8",
    "property": "#EAE2D8",
    "variable.readonly": "#E0B62C",
    "function": { "foreground": "#3F75B6", "bold": true },
    "method": "#3F75B6",
    "macro": "#C84C3A",
    "event": "#C84C3A",
    "decorator": "#5DCFA3",
    "regexp": "#5DCFA3",
    "string": "#4CBF6E",
    "number": "#E0B62C",
    "comment": "#7A7568",
    "operator": "#C8BEB4",
    "keyword": { "foreground": "#C84C3A", "bold": true },
    "modifier": "#C84C3A",
    "deprecated": { "strikethrough": true }
  }
}
