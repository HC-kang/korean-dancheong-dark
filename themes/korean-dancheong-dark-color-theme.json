    "notifications.foreground": "#EAE2D8",

    "minimap.findMatchHighlight": "#FAD01B33",
    "minimap.selectionHighlight": "#2D8B6F44",
    "minimap.errorHighlight": "#F55C48",
    "minimap.warningHighlight": "#FAD01B",

    "editorWidget.background": "#171E1A",
    "editorWidget.border": "#272F2B",
    "peekViewEditor.background": "#171E1A",
    "peekViewResult.background": "#0C120E",

    "diffEditor.insertedTextBackground": "#27A85122",
    "diffEditor.removedTextBackground": "#F55C4822",
    "diffEditor.diagonalFill": "#272F2B",

