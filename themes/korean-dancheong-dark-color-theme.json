    "sideBar.background": "#0C0A09",
    "sideBar.foreground": "#D4C8BF",
    "sideBarSectionHeader.background": "#141210",
    "sideBarTitle.foreground": "#E8DDD4",

    "tab.activeBackground": "#0F0D0B",
    "tab.inactiveBackground": "#0A0807",
    "tab.activeForeground": "#E8DDD4",
    "tab.inactiveForeground": "#8B7F75",
    "tab.border": "#2A2420",
    "tab.activeBorder": "#E8745C",

    "titleBar.activeBackground": "#141210",
