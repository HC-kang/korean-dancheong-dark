    "terminal.ansiBrightRed": "#FF8A73",
    "terminal.ansiBrightGreen": "#8AB494",
    "terminal.ansiBrightYellow": "#E8C166",
    "terminal.ansiBrightBlue": "#7DB8D4",
    "terminal.ansiBrightMagenta": "#B89CC4",
    "terminal.ansiBrightCyan": "#8DDCC0",
    "terminal.ansiBrightWhite": "#F5F0E8"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#6B5F55" }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": { "foreground": "#8B7F75" }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": { "foreground": "#CC4444" }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": { "foreground": "#B8A89C" }
    },

    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": { "foreground": "#E8745C", "fontStyle": "" }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": { "foreground": "#5A9FBF", "fontStyle": "" }
    },
    {
      "name": "Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": { "foreground": "#D4A544" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": { "foreground": "#E8DDD4" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#5A9FBF" }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": { "foreground": "#9B7EAE", "fontStyle": "" }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": { "foreground": "#CC4444" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": { "foreground": "#D4A544" }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": { "foreground": "#6B9178" }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": { "foreground": "#6EC4A7" }
    },
    {
      "name": "Symbols",
      "scope": "constant.other.symbol",
      "settings": { "foreground": "#D4A544" }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": { "foreground": "#B8A89C" }
    },
    {
      "name": "HTML: Tags",
      "scope": ["meta.tag"],
      "settings": { "foreground": "#5A9FBF" }
    },
    {
      "name": "HTML: Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#6EC4A7", "fontStyle": "" }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#D4A544" }
    },
    {
      "name": "CSS: Values",
      "scope": ["support.constant.property-value"],
      "settings": { "foreground": "#6B9178" }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": { "foreground": "#E8745C", "fontStyle": "bold" }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": { "foreground": "#5A9FBF" }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": { "foreground": "#6B9178" }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": { "foreground": "#E8745C" }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": { "foreground": "#5A9FBF" }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#9B7EAE",
    "class": { "foreground": "#9B7EAE" },
    "interface": { "foreground": "#9B7EAE" },
    "enum": { "foreground": "#D4A544" },
    "enumMember": "#D4A544",
    "typeParameter": "#5A9FBF",
    "parameter": "#E8DDD4",
    "property": "#E8DDD4",
    "variable.readonly": "#D4A544",
    "function": { "foreground": "#5A9FBF" },
    "method": "#5A9FBF",
    "macro": "#E8745C",
    "event": "#E8745C",
    "decorator": "#6EC4A7",
    "regexp": "#6EC4A7",
    "string": "#6B9178",
    "number": "#D4A544",
    "comment": "#6B5F55",
    "operator": "#B8A89C",
    "keyword": { "foreground": "#E8745C" },
    "modifier": "#E8745C",
    "deprecated": { "strikethrough": true }
  }
}
