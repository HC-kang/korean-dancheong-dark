    "terminal.ansiRed": "#E8745C",
    "terminal.ansiGreen": "#6B9178",
    "terminal.ansiYellow": "#D4A544",
    "terminal.ansiBlue": "#5A9FBF",
    "terminal.ansiMagenta": "#9B7EAE",
    "terminal.ansiCyan": "#6EC4A7",
    "terminal.ansiWhite": "#D4C8BF",
