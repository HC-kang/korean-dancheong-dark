    "editor.wordHighlightBackground": "#27A85133",
    "editor.wordHighlightStrongBackground": "#27A85144",
    "editor.lineHighlightBackground": "#171E1A",
    "editor.inactiveSelectionBackground": "#0D49BB22",
    "editor.findMatchHighlightBackground": "#FAD01B33",
    "editorLink.activeForeground": "#4A8FD8",

    "editorGutter.addedBackground": "#27A851",
