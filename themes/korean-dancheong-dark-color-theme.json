    "activityBarBadge.background": "#F55C48",
    "activityBarBadge.foreground": "#FFFFFF",

    "sideBar.background": "#0C120E",
    "sideBar.foreground": "#DAD2C8",
    "sideBarSectionHeader.background": "#131A16",
    "sideBarTitle.foreground": "#EAE2D8",

    "tab.activeBackground": "#101612",
    "tab.inactiveBackground": "#080E0A",
    "tab.activeForeground": "#EAE2D8",
    "tab.inactiveForeground": "#9A9588",
    "tab.border": "#272F2B",
    "tab.activeBorder": "#F55C48",

    "titleBar.activeBackground": "#131A16",
    "titleBar.activeForeground": "#EAE2D8",
    "titleBar.inactiveForeground": "#8A8578",

    "statusBar.background": "#131A16",
    "statusBar.foreground": "#EAE2D8",
    "statusBar.noFolderBackground": "#171E1A",
    "statusBar.debuggingBackground": "#FAD01B",
    "statusBar.debuggingForeground": "#101612",

    "panel.background": "#101612",
    "panel.border": "#272F2B",

    "list.activeSelectionBackground": "#2D8B6F44",
    "list.activeSelectionForeground": "#EAE2D8",
    "list.activeSelectionIconForeground": "#EAE2D8",
    "list.inactiveSelectionBackground": "#0D49BB22",
    "list.hoverBackground": "#0D49BB1A",
    "list.errorForeground": "#F55C48",
    "list.warningForeground": "#FAD01B",
    "list.focusBackground": "#2D8B6F44",

    "breadcrumb.foreground": "#8A8578",
    "breadcrumb.focusForeground": "#4A8FD8",

    "badge.background": "#F55C48",
    "badge.foreground": "#FFFFFF",

    "button.background": "#F55C48",
    "button.foreground": "#FFFFFF",
